#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/pranav/Documents/Coding/Projects/username/node_modules/.pnpm/rolldown-vite@7.1.12/node_modules/rolldown-vite/bin/node_modules:/home/pranav/Documents/Coding/Projects/username/node_modules/.pnpm/rolldown-vite@7.1.12/node_modules/rolldown-vite/node_modules:/home/pranav/Documents/Coding/Projects/username/node_modules/.pnpm/rolldown-vite@7.1.12/node_modules:/home/pranav/Documents/Coding/Projects/username/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/pranav/Documents/Coding/Projects/username/node_modules/.pnpm/rolldown-vite@7.1.12/node_modules/rolldown-vite/bin/node_modules:/home/pranav/Documents/Coding/Projects/username/node_modules/.pnpm/rolldown-vite@7.1.12/node_modules/rolldown-vite/node_modules:/home/pranav/Documents/Coding/Projects/username/node_modules/.pnpm/rolldown-vite@7.1.12/node_modules:/home/pranav/Documents/Coding/Projects/username/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
